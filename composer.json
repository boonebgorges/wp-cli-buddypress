{
  "name": "buddypress/wp-cli-buddypress",
  "type": "wp-cli-package",
  "description": "WP-CLI Community Package of BuddyPress commands",
  "homepage": "https://github.com/buddypress/wp-cli-buddypress",
  "license": "MIT",
  "keywords": [
    "wp-cli",
    "buddypress",
    "community",
    "wordpress",
    "bp"
  ],
  "authors": [
    {
      "name": "The BuddPress Contributors",
      "homepage": "https://buddypress.org/"
    }
  ],
  "support": {
    "issues": "https://github.com/buddypress/wp-cli-buddypress/issues",
    "source": "https://github.com/buddypress/wp-cli-buddypress"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "wp-cli/wp-cli": "^2"
  },
  "require-dev": {
    "wp-cli/scaffold-command": "^1.2 || ^2",
    "wp-cli/extension-command": "^2.0",
    "wp-cli/entity-command": "^2.0",
    "wp-cli/wp-cli-tests": "^2.1.3"
  },
  "scripts": {
    "behat": "run-behat-tests",
    "behat-rerun": "rerun-behat-tests",
    "lint": "run-linter-tests",
    "phpcs": "run-phpcs-tests",
    "prepare-tests": "install-package-tests",
    "test": [
      "@lint",
      "@phpcs",
      "@behat"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "files": [
      "wp-cli-bp.php"
    ]
  },
  "config": {
    "platform": {
      "php": "5.6"
    },
    "process-timeout": 1800,
    "sort-packages": true
  },
  "extra": {
    "bundled": true,
    "commands": [
      "bp",
      "bp activity",
      "bp activity favorite",
      "bp group",
      "bp group meta",
      "bp group member",
      "bp group invite",
      "bp message",
      "bp email",
      "bp component",
      "bp member",
      "bp notification",
      "bp tool",
      "bp signup",
      "bp xprofile",
      "bp xprofile field",
      "bp xprofile group",
      "bp xprofile data"
    ]
  }
}
